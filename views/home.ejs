<%- include("partials/header"); %>

<h1>Home</h1>

<p><%= homeContent %></p>

<% posts.forEach(post => { %>
    <h1> <%= post.title %> </h1>
    <p>
        <%= post.content.slice(0, 100) + "..."; %> 
        <a href=<%= `/posts/${_.kebabCase(post.title)}` %>> Read more</a>
    </p>
<% }) %>

<%- include("partials/footer"); %>